{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "globals": {
    "fetch": false,
    "Promise": false,
    //"FormData": false,
    "test": false,
    "expect": false,
    "jest": false,
    "describe": false
  },
  "rules": {
    "no-console": 0
  }
}
